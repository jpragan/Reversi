

public class Board {
	
	
	Cell[][] board = new Cell [9][];
	
	public Board()
	{
		Cell C = new Cell();
		int j = 4;
		for ( int i = 0; i < 5; i++)
		{
			board[i] = new Cell[j];
			//board[i][j] = C;
			j++;
		}

		j = 7;
		for ( int i = 5; i < 9; i++)
		{
			board[i] = new Cell[j];
			//board[i][j] = C;
			j--;
		}
		
	}

	public String toString()
	{
		return("    [" + board[0][0] + "][" + board[0][1] + "][" + board[0][2] + "][" + board[0][3] + "][" + board[0][4] +"] \n" +
			   "   [" + board[1][0] + "][" + board[1][1] + "][" + board[1][2] + "][" + board[1][3] + "][" + board[1][4] + "][" + board[1][5] + "]");
	}

}
