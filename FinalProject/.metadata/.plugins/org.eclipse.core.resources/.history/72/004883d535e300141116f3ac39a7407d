

public class Board {
	
	
	Cell[][] board = new Cell [9][];
	
	public Board()
	{
		int j = 5;
		for ( int i = 0; i < 5; i++)
		{
			board[i] = new Cell[j];
			j++;
		}

		j = 7;
		for ( int i = 5; i < 9; i++)
		{
			board[i] = new Cell[j];
			j--;
		}
	}

	public String toString()
	{
		return("    [" + board[0][0] + "][" + board[0][1] + "][" + board[0][2] + "][" + board[0][3] + "][" + board[0][4] +
			   "   [" + board[1][0] + "][" + board[1][1] + "][" + board[1][2] + "][" + board[1][3] + "][" + board[1][4] + "][" + board[1][5]);
	}

}
